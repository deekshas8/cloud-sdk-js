{
  "name": "@sap-cloud-sdk/internal-test-utils",
  "version": "2.3.0",
  "description": "Shared utils for testing",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "compile": "tsc -b",
    "lint": "prettier . --config='../../.prettierrc' --ignore-path='../../.prettierignore' -c",
    "lint:fix": "prettier . --config='../../.prettierrc' --ignore-path='../../.prettierignore' -w",
    "check:dependencies": "depcheck .",
    "check:public-api": "ts-node ../../scripts/check-public-api-cli.ts"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "jsonwebtoken": "^8.5.1",
    "nock": "^13.0.11",
    "typescript": "~4.6.2"
  }
}
